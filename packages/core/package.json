{
  "name": "@junobuild/core",
  "version": "0.0.7",
  "description": "JavaScript core client for Juno",
  "author": "David Dal Busco (https://daviddalbusco.com)",
  "license": "MIT",
  "main": "dist/index.umd.cjs",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "rmdir": "node ../../scripts/rmdir.mjs",
    "build": "npm run rmdir && mkdir -p dist && npm run vite && cp -R declarations dist && cp -R types dist",
    "prepack": "npm run build",
    "vite": "tsc && vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildwithjuno/juno-js.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/buildwithjuno/juno-js"
  },
  "keywords": [
    "blockchain-as-a-service",
    "baas",
    "dapps",
    "dapps-development",
    "internet computer",
    "smart-contracts",
    "web3"
  ],
  "homepage": "https://juno.build",
  "dependencies": {
    "@dfinity/agent": "^0.15.4",
    "@dfinity/auth-client": "^0.15.4",
    "@dfinity/identity": "^0.15.4",
    "@dfinity/principal": "^0.15.4"
  }
}
